#include<iostream>
using namespace std;
int main()
{
    //derefrencing the data of element 
    int a[10];
    a[0]=13;
    a[1]=3;
    a[2]=33;
    a[3]=133;
    a[4]=123;
    cout<<a<<endl;
    cout<<*a<<endl;
    cout<<*(a+1)<<endl;
    cout<<*(a+2)<<endl;
    cout<<*(a+3)<<endl;
    cout<<*(a+4)<<endl;
return 0;
}
